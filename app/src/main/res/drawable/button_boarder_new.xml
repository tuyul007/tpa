<selector xmlns:android="http://schemas.android.com/apk/res/android" >
<item android:state_pressed="true" >
    <shape>
        <solid android:color="#3E5AB7" />
        <stroke
            android:width="3dp"
            android:color="#242424" />
        <corners
            android:radius="5dp" />
    </shape>
</item>
<item>
<shape>
    <solid android:color="#597FFA" />
    <stroke
        android:width="2dp"
        android:color="#242424" />
    <corners
        android:radius="5dp" />
</shape>
</item>
    </selector>